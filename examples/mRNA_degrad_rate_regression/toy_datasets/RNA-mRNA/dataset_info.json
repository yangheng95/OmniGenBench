{
  "dataset_name": "RNA-mRNA Degradation Prediction",
  "dataset_version": "1.0.0",
  "OmniGenBench": "v0.3.20alpha",
  "description": "A dataset for predicting RNA degradation rates in different conditions. The task is to predict reactivity, degradation rates at Mg_pH10, pH10, Mg_50C, and 50C conditions for each nucleotide position in RNA sequences.",
  "task_type": "token_regression",
  "domain": "RNA",
  "source": "OpenVaccine: COVID-19 mRNA Vaccine Degradation Prediction",
  "license": "CC BY 4.0",
  "citation": "@misc{openVaccine2020,\n  title={OpenVaccine: COVID-19 mRNA Vaccine Degradation Prediction},\n  author={OpenVaccine Team},\n  year={2020},\n  url={https://www.kaggle.com/competitions/stanford-covid-vaccine}\n}",
  "statistics": {
    "total_samples": 1918,
    "train_samples": 1727,
    "test_samples": 191,
    "avg_sequence_length": 107,
    "max_sequence_length": 107,
    "min_sequence_length": 107
  },
  "features": {
    "input": {
      "sequence": {
        "type": "string",
        "description": "RNA nucleotide sequence",
        "alphabet": ["A", "U", "G", "C"],
        "example": "GGAAAUUUUUUUUUUUUUUUUUUUUUUUUUUUU..."
      },
      "structure": {
        "type": "string",
        "description": "RNA secondary structure in dot-bracket notation",
        "alphabet": [".", "(", ")"],
        "example": "....(((((((....)))))))..........."
      },
      "predicted_loop_type": {
        "type": "string",
        "description": "Predicted loop type for each position",
        "alphabet": ["E", "S", "H", "I", "M", "B", "X"],
        "mapping": {
          "E": "External loop",
          "S": "Stem",
          "H": "Hairpin loop",
          "I": "Internal loop",
          "M": "Multiloop",
          "B": "Bulge",
          "X": "Unknown"
        }
      }
    },
    "output": {
      "reactivity": {
        "type": "float_array",
        "description": "Reactivity values for each nucleotide position",
        "range": "[-100, 100]",
        "special_values": {
          "-100": "Missing/Invalid value"
        }
      },
      "deg_Mg_pH10": {
        "type": "float_array",
        "description": "Degradation rate in Mg_pH10 condition"
      },
      "deg_pH10": {
        "type": "float_array",
        "description": "Degradation rate in pH10 condition"
      },
      "deg_Mg_50C": {
        "type": "float_array",
        "description": "Degradation rate in Mg_50C condition"
      },
      "deg_50C": {
        "type": "float_array",
        "description": "Degradation rate in 50C condition"
      },
      "reactivity_error": {
        "type": "float_array",
        "description": "Error estimates for reactivity predictions"
      },
      "deg_error_Mg_pH10": {
        "type": "float_array",
        "description": "Error estimates for Mg_pH10 degradation predictions"
      },
      "deg_error_pH10": {
        "type": "float_array",
        "description": "Error estimates for pH10 degradation predictions"
      },
      "deg_error_Mg_50C": {
        "type": "float_array",
        "description": "Error estimates for Mg_50C degradation predictions"
      },
      "deg_error_50C": {
        "type": "float_array",
        "description": "Error estimates for 50C degradation predictions"
      }
    },
    "metadata_fields": {
      "index": {
        "type": "integer",
        "description": "Sample index in the original dataset"
      },
      "id": {
        "type": "string",
        "description": "Unique identifier for the RNA sequence"
      },
      "signal_to_noise": {
        "type": "float",
        "description": "Signal-to-noise ratio of the measurement"
      },
      "SN_filter": {
        "type": "float",
        "description": "Signal-to-noise filter threshold"
      },
      "seq_length": {
        "type": "integer",
        "description": "Total sequence length"
      },
      "seq_scored": {
        "type": "integer",
        "description": "Number of positions with valid scores"
      }
    }
  },
  "preprocessing": {
    "tokenization": "Single nucleotide tokenization",
    "padding": "Max length padding to 107",
    "special_tokens": ["[CLS]", "[SEP]", "[PAD]", "[MASK]"]
  },
  "evaluation_metrics": {
    "primary": "MCRMSE",
    "description": "Mean Column-wise Root Mean Squared Error across all target columns",
    "target_columns": ["reactivity", "deg_Mg_pH10", "deg_Mg_50C"]
  },
  "data_splits": {
    "train": {
      "file": "train.json",
      "samples": 1727,
      "description": "Training set"
    },
    "test": {
      "file": "test.json",
      "samples": 191,
      "description": "Test set"
    }
  },
  "keywords": ["RNA", "degradation", "vaccine", "COVID-19", "mRNA", "stability prediction", "token regression"],
  "maintainer": {
    "name": "YANG, HENG",
    "email": "hy345@exeter.ac.uk",
    "github": "https://github.com/yangheng95"
  },
  "created_date": "2024-04-26",
  "last_updated": "2025-10-13",
  "format_version": "1.0",
  "notes": [
    "This dataset is used for predicting RNA degradation rates under various experimental conditions.",
    "The target values are per-nucleotide regression values.",
    "Special value -100 indicates missing or invalid measurements.",
    "The dataset includes error estimates for uncertainty quantification."
  ]
}
